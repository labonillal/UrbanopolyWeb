extends layout

block content
	#header-2
		a(href="/profile")
			h1 Urbanopoly
		#user
			img(src= user_pic)
			p #{user_name}
			a(href="/")
				p Logout

	#content
		div(class="panel panel-default")
			div(class="panel-heading" style="text-align:center")
				img(src='/images/title.png')
			div(class="panel-body")
				div(class="panel panel-primary")
					div(class="panel-heading")
						h3(style="text-align:center") Related Links
					div(class="panel-body")
						div(class="row" style="text-align:center;font-size:20px")
							a(href="#") Questionnaire
						div(class="row" style="text-align:center;font-size:20px")
							a(href="http://swa.cefriel.it/urbangames/urbanopoly/index.html") Web Page
						div(class="row" style="text-align:center;font-size:20px")
							a(href="http://swa.cefriel.it/urbangames/urbanopoly/terms.html")	Terms and Conditions
				div(class="panel panel-primary")
					div(class="panel-heading")
						h3(style="text-align:center") Partners
					div(class="panel-body")
						div(class="row")
							div(class="col-md-6" style="text-align:center")
								img(src='/images/credits/partner_cefriel.png')
							div(class="col-md-6" style="text-align:center")
								img(src='/images/credits/partner_poli.png')
						div(class="row")
							div(class="col-md-6" style="text-align:center")
								img(src='/images/credits/partner_planet.png')
							div(class="col-md-6" style="text-align:center")
								img(src='/images/credits/partner_7.png')
				div(class="panel panel-primary")
					div(class="panel-heading")
						h3(style="text-align:center") Team
					div(class="panel-body")
						div(class="row")
							div(class="col-md-6" style="text-align:center")
								h4 Irene Celino
							div(class="col-md-6" style="text-align:center")
								h4 Daniele Dell'Aglio
						div(class="row")
							div(class="col-md-6" style="text-align:center")
								h4 Dario Cerizza
							div(class="col-md-6" style="text-align:center")
								h4 Emanuele Della Valle
						div(class="row")
							div(class="col-md-6" style="text-align:center")
								h4 Simone Contessa
							div(class="col-md-6" style="text-align:center")
								h4 Stefano Fumeo
						div(class="row")
							div(class="col-md-6" style="text-align:center")
								h4 Marta Corubolo
							div(class="col-md-6" style="text-align:center")
								h4 Federico Piccinini
						div(class="row")
							div(class="col-md-12" style="text-align:center")
								h4 Luis Alberto Bonilla Leon